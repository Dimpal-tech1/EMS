@model IEnumerable<App_Rev.Core.User>

@{
    ViewData["Title"] = "User Master";
}

<div class="text-center">
    <h1 class="display-4">User List</h1>
    <div>
        <a href="/Home/CreateOrEdit" type="button">Create User</a>
    </div>
  <div class="table-responsive">
      <table class="table table-bordered">
          <thead>
              <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Emp ID</th>
                  <th>Email</th>
                  <th>Password</th>
                  <th>Mobile</th>
                  <th>Role</th>
                  <th>Status</th>
              </tr>
          </thead>
          <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td> 
                        <td>@item.Name</td> 
                        <td>@item.EmpId</td> 
                        <td>@item.Email</td> 
                        <td>@item.Password</td> 
                        <td>@item.Mobile</td> 
                        <td>@item.Role</td> 
                        <td>@item.Status</td> 
                    </tr>
                }
          </tbody>
      </table>
  </div>
</div>
